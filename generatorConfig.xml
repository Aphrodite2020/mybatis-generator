<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	 
	<!--无Example等内容，简单版	-->
	<context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
	 
	 <!-- 有Example查询条件内容,拓展版
	<context id="testTables" targetRuntime="MyBatis3"> -->
	
	
	<!-- ########################################## -->
	
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		
		<!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
		<jdbcConnection 
			driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://lmdevnts0001:3306/leaderment" 
			userId="root"
			password="Ldm_1910%">
		</jdbcConnection>

		<!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和 
			NUMERIC 类型解析为java.math.BigDecimal -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- targetProject:生成Entity类的路径 -->
		<javaModelGenerator targetProject=".\src" targetPackage="com.leaderment.orm.pojo">
			<!-- enableSubPackages:是否让schema作为包的后缀 -->
			<property name="enableSubPackages" value="false" />
			<!-- 从数据库返回的值被清理前后的空格 -->
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
        <!-- targetProject:XXXMapper.xml映射文件生成的路径 -->
		<sqlMapGenerator targetProject=".\src" targetPackage="com.leaderment.orm.mapper">
			<!-- enableSubPackages:是否让schema作为包的后缀 -->
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		
		<!-- targetPackage：Mapper接口生成的位置 -->
		<javaClientGenerator type="XMLMAPPER" targetProject=".\src" targetPackage="com.leaderment.orm.mapper">
			<!-- enableSubPackages:是否让schema作为包的后缀 -->
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>
		
		<!-- 数据库表名字和我们的entity类对应的映射指定 -->
		<table tableName="fulfillment_in_warehouse" domainObjectName="FulfillmentInWarehouse"/>
		<!-- <table tableName="counterfeit_seller_orders" domainObjectName="CounterfeitSellerOrders"/>
		<table tableName="counterfeit_seller_purchase_account" domainObjectName="CounterfeitSellerPurchaseAccount"/>
		<table tableName="countries" domainObjectName="Countries"/> -->
	</context>
</generatorConfiguration>
